{
  "name": "ts-babel",
  "version": "1.0.0",
  "description": "Use Typescript with Babel",
  "main": "index.js",
  "scripts": {
    "tsc-only": "tsc --target es5 --module commonjs --outDir tsc-only",
    "tsc-babel": "tsc --target es6 --outDir tsc-babel-intermediate && babel tsc-babel-intermediate --out-dir tsc-babel"
  },
  "keywords": [
    "Typescript",
    "Babel",
    "ES6"
  ],
  "author": "kimamula",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.6.14",
    "typescript": "^1.5.0-beta"
  }
}
